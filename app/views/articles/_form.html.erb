<%= form_for @article do |f| %>

    <p>
      <%= f.label :title %><br>
      <%= f.text_field :title %>
    </p>

    <p>
      <%= f.label :content %><br>
      <%= f.text_area :content %>
    </p>

    <p>
      <%= f.label :youtube_url %><br>
      <%= f.url_field :youtube_url %>
    </p>

    <p>
      <%= f.submit :"作成", :class => "btn btn-primary" %>
    </p>

<% end %>

