<h2>フォロワー</h2>

<table class="table table-hover">
  <% @user.follower.each do |user| %>
      <tr>
        <td><%= image_tag user.image_url(:thumb_i) %>
          <%= link_to user.name, user_path(user.id) %></td>
      </tr>
  <% end %>
</table>

<div><%= link_to "戻る", user_path(@user.id) %></div>



